const data = require('fs').readFileSync('1.dat', 'utf-8').split('\n').filter(n => n).map((e) => Number(e));
const result = data.map((d1) => {
  return data.filter((d2) => {
    d1 + d1 == 2020

    if (d1 + d2 == 2020) {
      console.log([d1, d2])
      return [d1, d2]
    }
  });
});
console.log(JSON.stringify(result))


